    "@typescript-eslint/parser": "^5.62.0",
    "ajv": "^8.12.0",
    "axios": "^1.5.0",
    "crypto": "^1.0.1",
    "esbuild": "0.19.2",
    "eslint": "^8.48.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "git-hook-tasks": "ncpa0cpl/git-hook-tasks",
    "husky": "^8.0.3",
    "jest": "^29.6.4",
    "pr-changelog-gen": "^1.1.3",
    "prettier": "^3.0.3",
    "prettier-plugin-jsdoc": "^1.0.2",
    "typescript": "^5.2.2"
  },
