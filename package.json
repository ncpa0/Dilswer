    "ajv": "^8.12.0",
    "axios": "^1.4.0",
    "crypto": "^1.0.1",
    "esbuild": "0.18.16",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^5.0.0",
    "git-hook-tasks": "ncpa0cpl/git-hook-tasks",
    "husky": "^8.0.3",
    "jest": "^29.6.1",
    "pr-changelog-gen": "^1.1.3",
    "prettier": "^3.0.0",
    "prettier-plugin-jsdoc": "^0.4.2",
    "typescript": "^5.1.6"
